FEEDSTRINGS;
SKIPCHECK;
['SSL'] = S: IF(DB('SYS_Environments',!Environments,'Cloud') @= 'True', 'True', STET);
['Verify'] = S: IF(DB('SYS_Environments',!Environments,'Cloud') @= 'True', 'True', '');
['Async-Requests-Mode']= S: IF(DB('SYS_Environments',!Environments,'Cloud') @= 'True', 'True', '');
['Namespace']= S: IF(DB('SYS_Environments',!Environments,'Cloud') @= 'True', 'LDAP', STET);
['Port']= S: IF(DB('SYS_Environments',!Environments,'Cloud') @= 'True', '', STET);
['Gateway']= S: IF(DB('SYS_Environments',!Environments,'Cloud') @= 'True', '', STET);
['Base URL'] = S: IF(DB('SYS_Environments',!Environments,'Cloud') @= 'True', DB('SYS_Environments',!Environments,'Address') | '/tm1/api/' | DB('SYS_Environments',!Environments,'Server Name'), '');

FEEDERS;
['Address'] => DB('SYS_Environments',!Environments,'Base URL');
['Server Name'] => DB('SYS_Environments',!Environments,'Base URL');